{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "target": "es2019",
    "baseUrl": "src",
    "composite": true,
    "esModuleInterop": true,
    "paths": {
      "@utils/*":["./Utils/*"],
      "@type/*":["./types/*"]
    }
  },
  "include": ["src/**/*"]
}
