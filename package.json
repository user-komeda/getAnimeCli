{
  "name": "test",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "user_komeda @user-komeda",
  "bin": {
    "test": "./bin/run"
  },
  "homepage": "https://github.com/user-komeda/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "user-komeda/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "@prisma/client": "^3.9.1",
    "@types/axios": "^0.14.0",
    "@types/inquirer": "^8.2.0",
    "@types/jsdom": "^16.2.14",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "axios": "^0.25.0",
    "cli-ux": "^6.0.9",
    "csv-writer": "^1.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-unicorn": "^40.1.0",
    "inquirer": "^8.2.0",
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.0",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.22",
    "chai": "^4",
    "eslint": "^8.8.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "husky": "^7.0.4",
    "install": "^0.13.0",
    "lint-staged": "^12.3.5",
    "mocha": "^9",
    "npm-run-all": "^4.1.5",
    "oclif": "^2",
    "prettier-eslint-cli": "^5.0.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "oclif": {
    "bin": "test",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc ",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier --write src/**/*",
    "lint-format": "run-s lint format",
    "scrape": "./bin/dev scrape",
    "getAnime": "./bin/dev getAnime",
    "exportCsv": "./bin/dev exportCsv",
    "addRecord": "./bin/dev addRecord"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/user-komeda/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "lint-staged": {
    "./src/**": [
      "yarn lint",
      "yarn format"
    ]
  },
  "types": "dist/index.d.ts"
}
